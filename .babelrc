{
  "env": {
    "development": {
      "presets": [
        "react",
        ["es2015", { "modules": false }]
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-class-properties",
        "transform-object-rest-spread"
      ],
    },
    "production": {
  	  "presets": [
  	    "react",
  	    ["es2015", { "modules": false }]
  	  ],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    }
  }
}
